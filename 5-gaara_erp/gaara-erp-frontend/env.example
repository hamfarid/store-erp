# Gaara ERP Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration (from Nginx config)
VITE_API_URL=http://localhost:5001/api
VITE_ML_API_URL=http://localhost:5101
VITE_AI_API_URL=http://localhost:5601

# App Configuration
VITE_APP_NAME=Gaara ERP
VITE_APP_VERSION=12.0.0
VITE_APP_LOCALE=ar
VITE_APP_TIMEZONE=Asia/Riyadh

# Feature Flags
VITE_ENABLE_AI=true
VITE_ENABLE_ML=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Authentication
VITE_AUTH_TOKEN_KEY=access_token
VITE_AUTH_REFRESH_KEY=refresh_token
VITE_SESSION_TIMEOUT=86400

# Development
VITE_DEBUG_MODE=false
VITE_SHOW_DEV_TOOLS=false
