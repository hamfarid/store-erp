# /home/ubuntu/image_search_integration/auto_learning/.env.example
# ملف مثال لمتغيرات البيئة لمديول البحث الذاتي الذكي
# انسخ هذا الملف إلى .env وقم بتعديل القيم حسب بيئتك

# إعدادات قاعدة البيانات
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/gaara_erp

# إعدادات الذاكرة المركزية
MEMORY_API_URL=http://localhost:8001/api/v1
MEMORY_API_KEY=your_memory_api_key_here

# إعدادات نظام A2A
A2A_API_URL=http://localhost:8002/api/v1
A2A_API_KEY=your_a2a_api_key_here

# إعدادات التطبيق
LOG_LEVEL=INFO
DEBUG=False
SECRET_KEY=your_secret_key_here

# إعدادات CORS
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# إعدادات محركات البحث
GOOGLE_API_KEY=your_google_api_key_here
BING_API_KEY=your_bing_api_key_here

# إعدادات Redis (للتخزين المؤقت وإدارة المهام)
REDIS_URL=redis://localhost:6379/0
