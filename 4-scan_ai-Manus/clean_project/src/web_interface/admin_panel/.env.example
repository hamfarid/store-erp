# Environment variables for the admin panel

# Disable TypeScript type checking for faster builds
TSC_COMPILE_ON_ERROR=true

# Skip TypeScript checking in node_modules
SKIP_PREFLIGHT_CHECK=true

# Generate source maps for better debugging
GENERATE_SOURCEMAP=true

# Enable fast refresh for React
FAST_REFRESH=true

# Disable ESLint warnings overlay
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=true

# TypeScript settings
TS_NODE_SKIP_IGNORE=true
TS_NODE_TRANSPILE_ONLY=true
TS_NODE_FILES=true

# TypeScript configuration
TS_NODE_COMPILER_OPTIONS={"module":"commonjs","target":"es2020"}

# React app configuration
REACT_APP_TITLE=Admin Panel
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Agricultural AI System Admin Panel

# API configuration (update as needed)
REACT_APP_API_URL=http://localhost:8000

# Development settings
BROWSER=none
PORT=3000

# Webpack configuration
WEBPACK_DISABLE_ESLINT_PLUGIN=false
